{
  "name": "code-flow",
  "displayName": "code-flow",
  "description": "Generate an interactive diagram that maps the flow between different places in your codebase.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-flow.add-node",
        "title": "Code Flow: Add node to diagram"
      },
      {
        "command": "code-flow.open-diagram",
        "title": "Code Flow: Open diagram"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "when": "editorHasSelection",
          "command": "code-flow.add-node"
        }
      ],
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "code-flow.add-node",
          "group": "code-flow"
        },
        {
          "command": "code-flow.open-diagram",
          "group": "code-flow"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.52.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.15.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  }
}
